{
  "name": "recursive-react-docs-ai",
  "version": "0.1.0",
  "description": "Recursively generate documentation for React components with AI-powered descriptions",
  "main": "dist/index.js",
  "bin": {
    "docs-by-zach": "./dist/cli/index.js",
    "docs-ui": "./generate-and-run-docs.sh"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "start": "node dist/cli/index.js",
    "dev": "ts-node src/cli/index.ts",
    "test": "jest",
    "prepare": "npm run build",
    "example": "ts-node examples/example-usage.ts",
    "docs-ui": "bash ./generate-and-run-docs.sh",
    "docs-ollama": "bash ./generate-docs-with-ollama.sh"
  },
  "keywords": [
    "react",
    "documentation",
    "ai",
    "generator",
    "cli"
  ],
  "author": "Zach",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.3",
    "http-server": "^14.1.1",
    "minimatch": "^9.0.3",
    "natural": "^8.0.1",
    "nodemon": "^3.0.1",
    "open": "^9.1.0",
    "openai": "^4.0.0",
    "ora": "^5.4.1",
    "react": "^18.2.0",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^18.2.0",
    "vector-storage": "^1.0.55"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.3",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "jest": "^29.6.2",
    "puppeteer": "^24.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=14"
  }
}
