{
  "name": "code-y",
  "version": "0.1.0",
  "description": "Recursively generate documentation for React components with AI-powered descriptions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "code-y": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "src/ui",
    "generate-and-run-docs.sh",
    "generate_docs_helper.sh",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "start": "node dist/cli/index.js",
    "dev": "ts-node src/cli/index.ts",
    "test": "jest",
    "prepare": "npm run build",
    "example": "ts-node examples/example-usage.ts",
    "docs-ui": "bash ./generate-and-run-docs.sh",
    "docs-ollama": "npm run build && bash ./generate-and-run-docs.sh --use-ollama",
    "docs": "bash ./generate-and-run-docs.sh",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "documentation",
    "ai",
    "generator",
    "cli"
  ],
  "author": "Zach",
  "license": "MIT",
